<?xml version="1.0" ?>
<robot name="robotic_arm">

    <!-- Define the base link -->
    <link name="base_link">
        <inertial>
            <mass value="3815"/>
            <origin xyz="0 0 0.309" rpy="0 0 0"/>
            <inertia ixx="36.11" ixy="0.0" ixz="0.0" iyy="36.11" iyz="0.0" izz="47.69"/>
        </inertial>
        <visual>
            <geometry>
                <mesh filename="roboticarm/meshes/baseMESH.stl" />
            </geometry>
            <material name="pink">
                <color rgba="246 36 89 1" />
            </material>
        </visual>
        <collision>
            <geometry>
                <mesh filename="roboticarm/meshes/baseMESH.stl" />
            </geometry>
        </collision>
    </link>

    <!-- Define the first arm link -->
    <link name="arm_link1">
        <inertial>
            <mass value="1297"/>
            <origin xyz="0.47 0 0.695" rpy="0 0 0"/>
            <inertia ixx="43.93" ixy="-0.000047" ixz="-30.7" iyy="67.71" iyz="-0.000068" izz="25.76"/>
        </inertial>
        <visual>
            <geometry>
                <mesh filename="roboticarm/meshes/primerbrazoMESH.stl" />
            </geometry>
            <material name="green">
                <color rgba="1 152 117 1" />
            </material>
        </visual>
        <collision>
            <geometry>
                <mesh filename="roboticarm/meshes/primerbrazoMESH.stl" />
            </geometry>
        </collision>
    </link>

    <!-- Define the second arm link -->
    <link name="arm_link2">
        <inertial>
            <mass value="8747"/>
            <origin xyz="0.94 0.125 0" rpy="0 0 0"/>
            <inertia ixx="0.9649" ixy="0.0" ixz="0.0" iyy="24.17" iyz="0.0" izz="24.19"/>
        </inertial>
        <visual>
            <geometry>
                <mesh filename="roboticarm/meshes/segundobrazoMESH.stl" />
            </geometry>
            <material name="yellow">
                <color rgba="216 250 8 1" />
            </material>
        </visual>
        <collision>
            <geometry>
                <mesh filename="roboticarm/meshes/segundobrazoMESH.stl" />
            </geometry>
        </collision>
    </link>

    <!-- Define the gripper link -->
    <link name="gripper_link">
        <inertial>
            <mass value="38.479"/>
            <origin xyz="0.013 0.125 0.125" rpy="0 0 0"/>
            <inertia ixx="0.0519" ixy="0.0" ixz="0.0" iyy="0.03277" iyz="0.0" izz="0.04381"/>
        </inertial>
        <visual>
            <geometry>
                <mesh filename="roboticarm/meshes/gripperMESH.stl" />
            </geometry>
            <material name="blue">
                <color rgba="129 207 224 1" />
            </material>
        </visual>
        <collision>
            <geometry>
                <mesh filename="roboticarm/meshes/gripperMESH.stl" />
            </geometry>
        </collision>
    </link>

    <!-- Define the joints -->
    <joint name="base_to_arm1" type="revolute">
        <origin xyz="0 0 0.625" rpy="0 0 0" />
        <parent link="base_link" />
        <child link="arm_link1" />
        <axis xyz="0 0 1" />
        <limit lower="-3.1416" upper="3.1416"/> 
    </joint>

    <joint name="arm1_to_arm2" type="revolute">
        <origin xyz="1.294 -0.125 1.84" rpy="0 0 0" />
        <parent link="arm_link1" />
        <child link="arm_link2" />
        <axis xyz="0 1 0" />
        <limit lower="-3.1416" upper="3.1416"/>
    </joint>

    <joint name="arm2_to_gripper" type="revolute">
        <origin xyz="1.85 0 -0.125" rpy="0 0 0" />
        <parent link="arm_link2" />
        <child link="gripper_link" />
        <axis xyz="1 0 0" />
        <limit lower="-3.1416" upper="3.1416"/>
    </joint>
</robot>
